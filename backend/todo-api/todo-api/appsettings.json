  "JwtSettings": {
    "Issuer": "bv_todo_app",
    "Audience": "bv_todo_users",
    "ExpirationMinutes": 60,
    "Secret": "default_secret_if_not_found_in_env"
  },
  "AllowedOrigins": "http://localhost:4200",
